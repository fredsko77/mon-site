<div class="modal" id="project-modal" tabindex="-1" aria-labelledby="project-create-modal" aria-hidden="true">
    <form action="{{ path('dashboard_project_new') }}" method="post" id="project-create-form">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="project-create-modal">Nouveau projet</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="name" class="form-label">Nom du projet</label>
                        <input type="text" name="name" id="name" class="form-control" placeholder="Le nom du projet" value="">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Fermer</button>
                    <button type="submit" class="btn btn-primary">Enregistrer</button>
                </div>
            </div>
        </div>
    </form>
</div>